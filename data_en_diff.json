      "views": 18,
      "lines": [
        {
          "text": "Weekly journal 2025-06-03~2025-06-17",
          "created": 1748882126,
          "updated": 1750141364,
          "userId": "582e63d27c56960011aff09e"
        },
        {
          "text": "prev  [Weekly note 2025-05-26~2025-06-03] ",
          "created": 1748882126,
          "updated": 1748882133,
          "userId": "582e63d27c56960011aff09e"
        },
        {
          "text": "",
          "created": 1748882128,
          "updated": 1748882128,
          "userId": "582e63d27c56960011aff09e"
        },
        {
          "text": "For 1 week",
          "created": 1748882144,
          "updated": 1748882147,
          "userId": "582e63d27c56960011aff09e"
