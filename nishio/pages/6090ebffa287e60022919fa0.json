    },
    {
      "id": "6090ec23aff09e00003d1b25",
      "text": " >|python|",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b26",
      "text": " import BPyAddMesh",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b27",
      "text": " import Blender",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b28",
      "text": " from math import cos, sin, pi",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b29",
      "text": " Vector = Blender.Mathutils.Vector",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b2a",
      "text": " ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b2b",
      "text": " def turtle():",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b2c",
      "text": "     verts = []",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b2d",
      "text": "     faces = []",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b2e",
      "text": "     context = dict(x=0, y=0, dir = (1, 0))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b2f",
      "text": "     verts.append(Vector(context[\"x\"], context[\"y\"], 0))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b30",
      "text": "     verts.append(Vector(context[\"x\"], context[\"y\"], 1))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b31",
      "text": "     ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b32",
      "text": "     def forward(mag):",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b33",
      "text": "         dir = context[\"dir\"]",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b34",
      "text": "         context[\"x\"] += dir[0] * mag ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b35",
      "text": "         context[\"y\"] += dir[1] * mag ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b36",
      "text": " ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b37",
      "text": "         verts.append(Vector(context[\"x\"], context[\"y\"], 0))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b38",
      "text": "         verts.append(Vector(context[\"x\"], context[\"y\"], 1))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b39",
      "text": "         i = len(verts)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b3a",
      "text": "         faces.append((i - 1, i - 2, i - 4, i - 3))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b3b",
      "text": " ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b3c",
      "text": "     def rot(rad):",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b3d",
      "text": "         dir = context[\"dir\"]",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b3e",
      "text": "         context[\"dir\"] = (",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b3f",
      "text": "             dir[0] * cos(rad) - dir[1] * sin(rad),",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b40",
      "text": "             dir[0] * sin(rad) + dir[1] * cos(rad))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b41",
      "text": "     ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b42",
      "text": " ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b43",
      "text": "     def deg(x):",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b44",
      "text": "         return 2 * pi / 360 * x",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b45",
      "text": " ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b46",
      "text": "     def koch(level=1):",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b47",
      "text": "         if level == 0:",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b48",
      "text": "             forward(1)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b49",
      "text": "         else:",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b4a",
      "text": "             koch(level - 1)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b4b",
      "text": "             rot(deg(60))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b4c",
      "text": "             koch(level - 1)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b4d",
      "text": "             rot(deg(-120))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b4e",
      "text": "             koch(level - 1)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b4f",
      "text": "             rot(deg(60))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b50",
      "text": "             koch(level - 1)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b51",
      "text": " ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b52",
      "text": "     koch(1)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b53",
      "text": "     rot(deg(-120))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b54",
      "text": "     koch(1)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b55",
      "text": "     rot(dega(-120))",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b56",
      "text": "     koch(1)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b57",
      "text": " ",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b58",
      "text": "     BPyAddMesh.add_mesh_simple('Turtle', verts, [], faces)",
      "created": 1243436400,
      "updated": 1243436400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d1b59",
      "text": " ",
