    {
      "id": "5e2e627d46424b001751bb1a",
      "text": "bs4",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098177,
      "updated": 1580098177
    },
    {
      "id": "5e2e62a6aff09e0000512251",
      "text": "[Beautiful Soup Documentation — Beautiful Soup 4.4.0 documentation https://www.crummy.com/software/BeautifulSoup/bs4/doc/]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098214,
      "updated": 1580098214
    },
    {
      "id": "5e2e6281aff09e0000512230",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098178,
      "updated": 1580098185
    },
    {
      "id": "5e2e6289aff09e0000512241",
      "text": " data = open(\"quotes/1.html\")",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098185,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610c5",
      "text": " soup = bs4.BeautifulSoup(data, \"lxml\")",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610c6",
      "text": "\t",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098898
    },
    {
      "id": "5e2e654daff09e00005610c7",
      "text": " xs = soup(\"blockquote\")",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610c8",
      "text": " for x in xs:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610c9",
      "text": "     quote = x.text.strip()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610ca",
      "text": "     ref = x.nextSibling.nextSibling(\"a\")[0]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610cb",
      "text": "     booktitle = ref.text.strip()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610cc",
      "text": "     s = ref.nextSibling",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610cd",
      "text": "     page = re.search(r\"(\\d+)ページ\", s).groups()[0]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610ce",
      "text": "     print(quote, booktitle, page)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580098893,
      "updated": 1580098896
    },
    {
      "id": "5e2e654daff09e00005610cf",
