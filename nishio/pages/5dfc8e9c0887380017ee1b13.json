    {
      "id": "5dfc8e9c0887380017ee1b13",
      "text": "キャンバス汚染解決編",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832672,
      "updated": 1576832673
    },
    {
      "id": "5dfc8ea0aff09e0000023a97",
      "text": "[キャンバス汚染]の解決編",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832673,
      "updated": 1576832679
    },
    {
      "id": "5dfc8ea7aff09e0000023a99",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832679,
      "updated": 1576832679
    },
    {
      "id": "5dfc8ea7aff09e0000023a9a",
      "text": "[Flask]で[プロキシサーバ]を書いた",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832679,
      "updated": 1576832730
    },
    {
      "id": "5dfc8eb9aff09e0000023a9b",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832697,
      "updated": 1576832699
    },
    {
      "id": "5dfc8ebbaff09e0000023a9c",
      "text": " import requests",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832700,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023a9d",
      "text": " from flask import Flask",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023a9e",
      "text": " from flask import Response",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023a9f",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa0",
      "text": " app = Flask(__name__)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa1",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa2",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa3",
      "text": " @app.route('/gyazo.com/<hash>')",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa4",
      "text": " def gyazo(hash):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa5",
      "text": "     url = f\"https://gyazo.com/{hash}/thumb/400\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa6",
      "text": "     r = requests.get(url, allow_redirects=True)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa7",
      "text": "     r = Response(r.content, mimetype=\"image/png\")",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa8",
      "text": "     r.headers['Access-Control-Allow-Origin'] = '*'",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aa9",
      "text": "     r.headers['Cache-Control'] = 'max-age = 31536000'",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ebdaff09e0000023aaa",
      "text": "     return r",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832701,
      "updated": 1576832701
    },
    {
      "id": "5dfc8ea1aff09e0000023a98",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1576832674,
      "updated": 1576832756
