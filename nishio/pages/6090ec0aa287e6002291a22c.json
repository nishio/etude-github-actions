      "updated": 1336662000,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec2eaff09e00003dbf8e",
      "text": " >>> get(\"Person$0\").join(get(\"love\"))",
      "created": 1336662000,
      "updated": 1336662000,
