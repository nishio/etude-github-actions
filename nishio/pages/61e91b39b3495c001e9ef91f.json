      "created": 1547018291,
      "updated": 1547018300,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "61e91b38aff09e0000d0afd9",
      "text": "\t\t[具体] = [concrete] phase",
      "created": 1547018301,
      "updated": 1553220715,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "61e91b38aff09e0000d0afda",
      "text": "\t\t[体験] = [experience]",
      "created": 1547018378,
      "updated": 1553220719,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "61e91b38aff09e0000d0afdb",
      "text": "\t\t[情報収集] = [information gathering]",
