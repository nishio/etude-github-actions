    },
    {
      "id": "5c5244b9aff09e0000e430ea",
      "text": "[https://gyazo.com/8c5ca07cf02c9097563acf1b6078ec37]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1548895418,
      "updated": 1548895419
    },
    {
      "id": "5c5244baaff09e0000e430ec",
