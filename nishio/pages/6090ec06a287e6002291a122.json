      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bb7",
      "text": " ",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bb8",
      "text": " # test fairness",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bb9",
      "text": " ary = [[0, 0]] # [[Tid, num], [Tid, num], ...]",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bba",
      "text": " (1..max).map{|i|",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bbb",
      "text": "   Thread.new(i){|tid|",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bbc",
      "text": "     lmax.times{",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bbd",
      "text": "       e = ary[-1]",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bbe",
      "text": "       if e[0] != tid",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bbf",
      "text": "         e = [tid, 0]",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc0",
      "text": "         ary << e",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc1",
      "text": "       end",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc2",
      "text": "       e[1] += 1",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc3",
      "text": "     }",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc4",
      "text": "   }",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc5",
      "text": " }.each{|t|",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc6",
      "text": "   t.join",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc7",
      "text": " }",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc8",
      "text": " ary.each{|(t, c)|",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bc9",
      "text": "   puts \"#{t}\\t#{c}\"",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bca",
      "text": " }",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bcb",
      "text": " puts ary.length",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7bcc",
      "text": " ||<",
      "created": 1291647600,
      "updated": 1291647600,
      "userId": "582e63d27c56960011aff09e"
    },
