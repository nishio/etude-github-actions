    },
    {
      "id": "6090ec29aff09e00003d719f",
      "text": " うむうむ。いい感じ。",
      "created": 1282575600,
      "updated": 1282575600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d71a0",
      "text": " ",
      "created": 1282575600,
      "updated": 1282575600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d71a1",
      "text": " しかし構文木の表示はうまくいかなかった。",
      "created": 1282575600,
      "updated": 1282575600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d71a2",
      "text": " ",
      "created": 1282575600,
      "updated": 1282575600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d71a3",
      "text": " >|python|",
      "created": 1282575600,
      "updated": 1282575600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d71a4",
      "text": " >>> 1",
      "created": 1282575600,
