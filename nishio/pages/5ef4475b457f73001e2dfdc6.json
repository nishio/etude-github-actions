    {
      "id": "5ef4475b457f73001e2dfdc6",
      "text": "ABC167D",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067359,
      "updated": 1593067362
    },
    {
      "id": "5ef4476aaff09e0000f4b1b6",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067370,
      "updated": 1593067372
    },
    {
      "id": "5ef4476caff09e0000f4b1b7",
      "text": " def solve(N, K, xs):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067373,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1b8",
      "text": "     \"void()\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1b9",
      "text": "     town_to_time = np.zeros(N, np.uint16)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1ba",
      "text": "     time_to_town = np.zeros(N, np.uint16)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1bb",
      "text": "     cur = 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1bc",
      "text": "     for i in range(K):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1bd",
      "text": "         cur = xs[cur - 1]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1be",
      "text": "         # print(town_to_time, time_to_town, cur)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1bf",
      "text": "         if town_to_time[cur - 1]:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c0",
      "text": "             # visited before",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c1",
      "text": "             period = i + 1 - town_to_time[cur - 1]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c2",
      "text": "             rest = K - i - 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c3",
      "text": "             rest %= period",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c4",
      "text": "             # print(rest, town_to_time[cur - 1], town_to_time[cur - 1] + rest)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c5",
      "text": "             print(time_to_town[town_to_time[cur - 1] + rest - 1])",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c6",
      "text": "             return",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c7",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c8",
      "text": "         town_to_time[cur - 1] = i + 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1c9",
      "text": "         time_to_town[i] = cur",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1ca",
      "text": "     print(cur)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067414
    },
    {
      "id": "5ef44796aff09e0000f4b1cb",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593067414,
      "updated": 1593067416
    },
    {
