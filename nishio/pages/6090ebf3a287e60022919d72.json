      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4369",
      "text": " \tprint x",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c436a",
      "text": " ",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c436b",
      "text": " \t",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c436c",
      "text": " >>> def f2(x = f1):",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c436d",
