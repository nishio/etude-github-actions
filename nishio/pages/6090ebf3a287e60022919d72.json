      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4372",
      "text": " >>> f2()",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4373",
      "text": " <function f1 at 0x01A07C30>",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4374",
      "text": " ||<",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4375",
      "text": " ",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4376",
      "text": " PHP:",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4377",
      "text": " >|php|",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4378",
      "text": " <?php",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4379",
      "text": " define(\"f1\", \"hoge\");",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c437a",
      "text": " ",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c437b",
      "text": " function f1($x=f1){",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c437c",
      "text": "     echo $x;",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c437d",
      "text": " }",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c437e",
      "text": " ",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c437f",
      "text": " function f2($x=f1){",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4380",
      "text": "     echo $x;",
      "created": 1193065200,
      "updated": 1193065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec18aff09e00003c4381",
