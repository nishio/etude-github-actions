      "id": "6090ec2daff09e00003db235",
      "text": " fact {",
      "created": 1333983600,
      "updated": 1333983600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec2daff09e00003db236",
      "text": " \tall s: State | s in StateManager.state",
      "created": 1333983600,
      "updated": 1333983600,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec2daff09e00003db237",
      "text": " }",
      "created": 1333983600,
      "updated": 1333983600,
      "userId": "582e63d27c56960011aff09e"
    },
