    },
    {
      "id": "5bd6d728aff09e000076a4b6",
      "text": "\tGameObject.Find(\"FooText\").GetComponent<Text>().text = x.ToString();",
      "created": 1531285432,
      "updated": 1531285443,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d728aff09e000076a4b7",
      "text": "テキスト自体に一意な名前\"FooText\"を振ってある場合。",
      "created": 1531285395,
      "updated": 1531285478,
      "userId": "582e63d27c56960011aff09e"
