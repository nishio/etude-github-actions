      "updated": 1589788597
    },
    {
      "id": "5ec23fb5aff09e000082bf8f",
      "text": " const paper = require(\"paper\");",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788597,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf90",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf91",
      "text": " function App() {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf92",
      "text": "   useEffect(() => {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf93",
      "text": "     var canvas = document.getElementById(\"myCanvas\");",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf94",
      "text": "     paper.setup(canvas);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf95",
      "text": "     var path = new paper.Path();",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf96",
      "text": "     path.strokeColor = \"black\";",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf97",
      "text": "     var start = new paper.Point(100, 100);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf98",
      "text": "     path.moveTo(start);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf99",
      "text": "     path.lineTo(start.add([200, -50]));",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf9a",
      "text": "     paper.view.draw();",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf9b",
      "text": "   }, []);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf9c",
      "text": "   return (",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf9d",
      "text": "     <div className=\"App\">",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf9e",
      "text": "       <div>",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bf9f",
      "text": "         <canvas",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bfa0",
      "text": "           id=\"myCanvas\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bfa1",
      "text": "           data-paper-resize=\"true\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bfa2",
      "text": "           style={{ height: \"100vh\", width: \"100vw\" }}",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bfa3",
      "text": "         ></canvas>",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bfa4",
      "text": "       </div>",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bfa5",
      "text": "     </div>",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bfa6",
      "text": "   );",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
      "id": "5ec23fb6aff09e000082bfa7",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1589788598,
      "updated": 1589788598
    },
    {
