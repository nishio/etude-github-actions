      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1baff09e00003c6af2",
      "text": " http://q.hatena.ne.jp/1201442424/139588/",
      "created": 1201705200,
      "updated": 1201705200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1baff09e00003c6af3",
      "text": " >>",
      "created": 1201705200,
      "updated": 1201705200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1baff09e00003c6af4",
      "text": " そんなことより dropdb",
      "created": 1201705200,
      "updated": 1201705200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1baff09e00003c6af5",
      "text": " ",
