      "text": "   public function eventhandler(e:Event):void{",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca252",
      "text": "     log(\"hi!\");",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca253",
      "text": "   }",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca254",
      "text": " }",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca255",
      "text": " ",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca256",
      "text": " class Bar extends Sprite{",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca257",
      "text": "   public function Bar(){",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca258",
      "text": "     graphics.beginFill(0x00FF00);",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca259",
      "text": "     graphics.drawRect(0, 0, 50, 50);",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca25a",
      "text": "     alpha = 0.5;",
      "created": 1210172400,
