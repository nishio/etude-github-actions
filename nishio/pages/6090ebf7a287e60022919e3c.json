      "text": "   import flash.events.*;",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca238",
      "text": "   public class Client extends Sprite {",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca239",
      "text": "     public function Client() {",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca23a",
      "text": "       graphics.beginFill(0x00FF00);",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca23b",
      "text": "       graphics.drawRect(0, 0, 100, 100);",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca23c",
      "text": "       alpha = 0.3;",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca23d",
      "text": "       var f:Foo = new Foo();",
      "created": 1210172400,
      "updated": 1210172400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003ca23e",
