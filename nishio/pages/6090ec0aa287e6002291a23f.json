    {
      "id": "6090ec2eaff09e00003dc505",
      "text": " $ git rm -f README.rst",
      "created": 1339254000,
      "updated": 1339254000,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec2eaff09e00003dc506",
      "text": " ||<",
      "created": 1339254000,
      "updated": 1339254000,
      "userId": "582e63d27c56960011aff09e"
