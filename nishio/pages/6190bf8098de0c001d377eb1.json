    {
      "id": "6190bf8098de0c001d377eb1",
      "text": "Minecraft Pomodoro",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876163,
      "updated": 1636876167
    },
    {
      "id": "6190bf87aff09e0000142999",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876167,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e000014299b",
      "text": " import argparse",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e000014299c",
      "text": " import socket",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e000014299d",
      "text": " from time import sleep",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e000014299e",
      "text": " from datetime import datetime",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e000014299f",
      "text": " import subprocess",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a0",
      "text": " # POMODORO = 25",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a1",
      "text": " # REST = 5",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a2",
      "text": " POMODORO = 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a3",
      "text": " REST = 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a4",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a5",
      "text": " SUNSET = 12000",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a6",
      "text": " DAY_START = 24000",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a7",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a8",
      "text": " def main():",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429a9",
      "text": "     HOST = \"localhost\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429aa",
      "text": "     PORT = 25575",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429ab",
      "text": "     PASSWORD = \"***\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876177
    },
    {
      "id": "6190bf8aaff09e00001429ac",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429ad",
      "text": "     while True:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429ae",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429af",
      "text": "         # sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b0",
      "text": "         # sock.connect((HOST, PORT))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b1",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b2",
      "text": "         try:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b3",
      "text": "             # Log in",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b4",
      "text": "             # result = login(sock, PASSWORD)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b5",
      "text": "             # if not result:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b6",
      "text": "             #     print(\"Incorrect rcon password\")",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b7",
      "text": "             #     return",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b8",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429b9",
      "text": "             tick = get_tick()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429ba",
      "text": "             request = f\"time set {tick}\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429bb",
      "text": "             # request = \"time query\"",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429bc",
      "text": "             print(f\"send {request}\")",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429bd",
      "text": "             # response = command_no_response(sock, request)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429be",
      "text": "             subprocess.check_call(f'mcrcon -s -p {PASSWORD} \"{request}\"', shell=True)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429bf",
      "text": "             sleep(1)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c0",
      "text": "         finally:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c1",
      "text": "             # sock.close()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c2",
      "text": "             pass",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c3",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c4",
      "text": " def get_tick():",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c5",
      "text": "     now = datetime.now()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c6",
      "text": "     minute = (now.minute + now.second / 60) % (POMODORO + REST)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c7",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c8",
      "text": "     if minute <= POMODORO:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429c9",
      "text": "         tick = minute * SUNSET / POMODORO",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429ca",
      "text": "     else:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429cb",
      "text": "         m = minute - POMODORO",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429cc",
      "text": "         tick = SUNSET + (DAY_START - SUNSET) * m / REST",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429cd",
      "text": "     return int(tick)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429ce",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429cf",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429d0",
      "text": " if __name__ == '__main__':",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429d1",
      "text": "     main()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1636876171,
      "updated": 1636876171
    },
    {
      "id": "6190bf8aaff09e00001429d2",
