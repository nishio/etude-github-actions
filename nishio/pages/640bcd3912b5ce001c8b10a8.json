    {
      "id": "640bcd3912b5ce001c8b10a8",
      "text": "蜃気楼のような本質の影",
      "userId": "582e63d27c56960011aff09e",
      "created": 1678495038,
      "updated": 1678495058
    },
    {
      "id": "640c6df3aff09e0000c4dfd3",
      "text": "",
