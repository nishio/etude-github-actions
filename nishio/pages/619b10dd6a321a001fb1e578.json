    {
      "id": "619b13feaff09e00003f6917",
      "text": " def to_str(bits):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1637553150,
      "updated": 1637553150
    },
    {
      "id": "619b13feaff09e00003f6918",
      "text": "     return \"\".join(str(i) for i in bits)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1637553150,
      "updated": 1637553150
    },
    {
      "id": "619b13feaff09e00003f6919",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1637553150,
      "updated": 1637553150
    },
    {
      "id": "619b13feaff09e00003f691a",
      "text": " ",
