    },
    {
      "id": "6090ec22aff09e00003cf662",
      "text": " 追記：",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf663",
      "text": " -startappでアプリケーションfoobarを作る",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf664",
      "text": " -myapp/urlauto.pyを見てまねをする",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf665",
      "text": " -foobar/url.pyを書く",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf666",
      "text": " -foobar/template/index.htmlをつくる",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf667",
      "text": " -foobar/views.pyを書く",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf668",
      "text": " -foobar/models.pyを書く",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf669",
      "text": " -/adminを見てみたけど、さっき作ったモデルが管理画面に出てこないぞ？",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf66a",
      "text": " -myapp/admin.pyを見てまねをする",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf66b",
      "text": " -管理画面に出た！",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf66c",
      "text": " -アプリを作る",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf66d",
      "text": " -manage.py update",
      "created": 1237388400,
      "updated": 1237388400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec22aff09e00003cf66e",
      "text": " -できた。",
