      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12aa",
      "text": "     (r'seat/(?P<seat_id>.*)/', 'seat'),",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
