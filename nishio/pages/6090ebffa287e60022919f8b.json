      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12ca",
      "text": "     'saichugen.api_views',",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12cb",
      "text": "     *api_urlpatterns",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12cc",
      "text": " )",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12cd",
      "text": " ||<",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12ce",
      "text": " ",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12cf",
      "text": " # views.py 一部抜粋",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d0",
      "text": " >|python|",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d1",
      "text": " def api_doc(request):",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d2",
      "text": "     import urls, api_views",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d3",
      "text": "     api_list = []",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d4",
      "text": "     for (url, view_name) in urls.api_urlpatterns:",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d5",
      "text": "         view = getattr(api_views, view_name)",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d6",
      "text": "         api_list.append(",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d7",
      "text": "             (url, view.__doc__))",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d8",
      "text": "                 ",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12d9",
      "text": "     return render_to_response(",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12da",
      "text": "         request, ",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12db",
      "text": "         'saichugen/api_doc.html',",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12dc",
      "text": "         dict(api_list=api_list))",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12dd",
      "text": " ||<",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12de",
      "text": " ",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
