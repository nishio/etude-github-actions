      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12bd",
      "text": " さっき書いたこれ、こうすればいいと思った。",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12be",
      "text": " # urls.py (前略)",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12bf",
      "text": " >|python|",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12c0",
      "text": " api_urlpatterns = (",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12c1",
      "text": "     (r'start_game/', 'start_game'),",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12c2",
      "text": "     (r'join/(?P<game_id>.*)/', 'join'),",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12c3",
      "text": "     (r'add_player/(?P<game_id>.*)/(?P<player_id>.*)/', 'add_player'),",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12c4",
      "text": "     (r'seat/(?P<seat_id>.*)/', 'seat'),",
      "created": 1241881200,
      "updated": 1241881200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d12c5",
      "text": "     (r'play/(?P<seat_id>.*)/', 'play'),",
      "created": 1241881200,
