    {
      "id": "609bba68aff09e0000626380",
      "text": "例",
      "userId": "582e63d27c56960011aff09e",
      "created": 1620818537,
      "updated": 1620818538
    },
    {
      "id": "609bba54aff09e000062637f",
      "text": "　「先生が教えてくれた」というグループだな",
      "userId": "582e63d27c56960011aff09e",
      "created": 1620818517,
      "updated": 1620818541
    },
    {
      "id": "609bba6eaff09e0000626381",
