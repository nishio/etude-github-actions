    },
    {
      "id": "5ee1f3daaff09e00005ffd58",
      "text": "　チームのパフォーマンスは",
      "userId": "582e63d27c56960011aff09e",
      "created": 1591866330,
      "updated": 1591866331
    },
    {
      "id": "5ee1f3dbaff09e00005ffd59",
      "text": "　　あまり相関しない",
      "userId": "582e63d27c56960011aff09e",
      "created": 1591866331,
      "updated": 1591866331
    },
    {
      "id": "5ee1f3dbaff09e00005ffd5a",
      "text": "　　　平均的な個人の知性",
      "userId": "582e63d27c56960011aff09e",
      "created": 1591866331,
      "updated": 1591866331
    },
    {
      "id": "5ee1f3dbaff09e00005ffd5b",
      "text": "　　　一番賢い人の知性",
      "userId": "582e63d27c56960011aff09e",
      "created": 1591866331,
      "updated": 1591866331
    },
    {
      "id": "5ee1f3dbaff09e00005ffd5c",
      "text": "　　相関する",
      "userId": "582e63d27c56960011aff09e",
      "created": 1591866331,
      "updated": 1591866331
    },
    {
      "id": "5ee1f3dbaff09e00005ffd5d",
      "text": "　　　[社会的シグナル]感受性",
      "userId": "582e63d27c56960011aff09e",
      "created": 1591866331,
      "updated": 1591866331
    },
    {
      "id": "5ee1f3dbaff09e00005ffd5e",
      "text": "　　　発言切り替えの平等さ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1591866331,
      "updated": 1591866331
    },
    {
      "id": "5ee1f3dbaff09e00005ffd5f",
      "text": "　　　女性比率",
