      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df145",
      "text": " など や",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df146",
      "text": " だろ でしょ",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df147",
      "text": " しかし そして",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df148",
      "text": " によって により",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df149",
      "text": " られ られる",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df14a",
      "text": " 考え 見",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df14b",
      "text": " 8 9",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df14c",
      "text": " へ ヘ",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df14d",
      "text": " 図 表",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df14e",
      "text": " たり ながら",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df14f",
      "text": " なかっ なかつ",
      "created": 1393426800,
      "updated": 1393426800,
      "userId": "582e63d27c56960011aff09e"
    },
