{
  "id": "5d2d8e74c51c9f0017e39634",
  "title": "Redux化",
  "created": 1563266679,
  "updated": 1563272715,
  "lines": [
    {
      "id": "5d2d8e74c51c9f0017e39634",
      "text": "Redux化",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266679,
      "updated": 1563266679
    },
    {
      "id": "5d2d8ea3aff09e0000130b12",
      "text": "こんな感じですかね。 [Redux]+[TypeScript]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266723,
      "updated": 1563272715
    },
    {
      "id": "5d2d8eabaff09e0000130b13",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266732,
      "updated": 1563266732
    },
    {
      "id": "5d2d8e76aff09e0000130ae4",
      "text": "code:ts",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266679,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130ae6",
      "text": " import { createStore } from 'redux';",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130ae7",
      "text": "\t...",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266699
    },
    {
      "id": "5d2d8e8aaff09e0000130b10",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266699,
      "updated": 1563266699
    },
    {
      "id": "5d2d8e83aff09e0000130aea",
      "text": " export const initialState = {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e90aff09e0000130b11",
      "text": "\t\t...",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266704,
      "updated": 1563266707
    },
    {
      "id": "5d2d8e83aff09e0000130aeb",
      "text": " };",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266704
    },
    {
      "id": "5d2d8e83aff09e0000130af5",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130af6",
      "text": " export type StateType = typeof initialState;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130af7",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130af8",
      "text": " enum ActionType {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130af9",
      "text": "   SET_ITEMS = \"SET_ITEMS\",",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130afa",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130afb",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130afc",
      "text": " export const setItems = (items: StateItem[]) => {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130afd",
      "text": "   return { type: ActionType.SET_ITEMS, payload: items }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130afe",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130aff",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b00",
      "text": " interface Action {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b01",
      "text": "   type: ActionType;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b02",
      "text": "   payload: any;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b03",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b04",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b05",
      "text": " function rootReducer(state = initialState, action: Action) {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b06",
      "text": "   switch (action.type) {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b07",
      "text": "     case ActionType.SET_ITEMS:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b08",
      "text": "       return { ...state, items: action.payload };",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b09",
      "text": "     default:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b0a",
      "text": "       return state;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b0b",
      "text": "   }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b0c",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b0d",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b0e",
      "text": " export const store = createStore(rootReducer);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266691
    },
    {
      "id": "5d2d8e83aff09e0000130b0f",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266691,
      "updated": 1563266720
    },
    {
      "id": "5d2d8e81aff09e0000130ae5",
      "text": "[pRegroup-done-2019]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1563266690,
      "updated": 1580046375
    },
    {
      "id": "5d2d8ec2aff09e0000130b14",
