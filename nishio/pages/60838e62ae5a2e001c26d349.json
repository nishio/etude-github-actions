    },
    {
      "id": "60838e62aff09e000054a245",
      "text": "仕事の進め方の一般教養",
      "userId": "582e63d27c56960011aff09e",
      "created": 1619234403,
      "updated": 1619234403
    },
    {
      "id": "60838e62aff09e000054a246",
