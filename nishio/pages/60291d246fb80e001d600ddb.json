      "text": " declare module 'reactn/default' {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616493760,
      "updated": 1616493760
    },
    {
      "id": "6059bcc0aff09e0000a663fc",
      "text": "   export interface State extends TYPE_GLOBAL_STATE { }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616493760,
      "updated": 1616493760
    },
    {
      "id": "6059bcc0aff09e0000a663fd",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616493760,
      "updated": 1616493760
    },
