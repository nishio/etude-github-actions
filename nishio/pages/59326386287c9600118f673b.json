      "userId": "582e63d27c56960011aff09e",
      "created": 1496477715,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c3090",
      "text": " {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c3091",
      "text": "   ergodox_led_all_off();",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c3092",
      "text": "   ergodox_led_all_set(LED_BRIGHTNESS_HI);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c3093",
      "text": "   i2c_init();",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477725
    },
    {
      "id": "59327013aff09e00004c3095",
      "text": "   ergodox_right_led_2_on();",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477726
    },
    {
      "id": "59327013aff09e00004c3097",
      "text": "   i2c_start(0x70);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477727
    },
    {
      "id": "59327013aff09e00004c3099",
      "text": "   ergodox_right_led_3_on();",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477728
    },
    {
      "id": "59327013aff09e00004c309b",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c309c",
      "text": "   i2c_write(0x21);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c309d",
      "text": "   i2c_stop();",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c309e",
      "text": "   _delay_ms(10);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c309f",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c30a0",
      "text": "   i2c_start(0x70);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c30a1",
      "text": "   i2c_write(0x81);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c30a2",
      "text": "   i2c_stop();",
      "userId": "582e63d27c56960011aff09e",
