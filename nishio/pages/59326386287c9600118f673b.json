    },
    {
      "id": "59327013aff09e00004c30a3",
      "text": "   _delay_ms(10);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c30a4",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c30a5",
      "text": "   i2c_start(0x70);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c30a6",
      "text": "   i2c_write(0xEF);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1496477716,
      "updated": 1496477716
    },
    {
      "id": "59327013aff09e00004c30a7",
      "text": "   i2c_stop();",
      "userId": "582e63d27c56960011aff09e",
