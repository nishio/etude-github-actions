      "id": "6090ec31aff09e00003df350",
      "text": " >|python|",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df351",
      "text": " # Python",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df352",
      "text": " class MyClass(object):",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df353",
      "text": "     @staticmethod",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df354",
      "text": "     def my_static_method():",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df355",
      "text": "         print \"I'm static method!\";",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df356",
      "text": " ",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df357",
