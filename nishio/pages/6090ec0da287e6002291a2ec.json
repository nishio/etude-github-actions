      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df344",
      "text": " ",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df345",
      "text": "     public static void main(String[] args){",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df346",
      "text": "         MyClass.my_static_method();",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df347",
      "text": "         // -> I'm static method!",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df348",
      "text": " ",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df349",
      "text": "         MyClass x = new MyClass();",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df34a",
      "text": "         x.my_instance_method();",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df34b",
      "text": "         // -> I'm instance method!",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df34c",
      "text": "     }",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec31aff09e00003df34d",
      "text": " };",
      "created": 1399561200,
      "updated": 1399561200,
      "userId": "582e63d27c56960011aff09e"
    },
