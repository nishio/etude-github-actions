    {
      "id": "5bd6d727aff09e000076a1ac",
      "text": " [シェーダ]を[Unlit/Texture]にするとよいらしい",
      "created": 1529647822,
      "updated": 1529647822,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a1ad",
      "text": "\tcode:cs",
      "created": 1529672697,
      "updated": 1529672712,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a1ae",
      "text": "  obj.GetComponent<Renderer>().material.shader = Shader.Find(\"Unlit/Texture\");",
      "created": 1529672702,
      "updated": 1529672703,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a1af",
      "text": "",
      "created": 1529647815,
