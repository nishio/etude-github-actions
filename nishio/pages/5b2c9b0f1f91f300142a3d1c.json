      "created": 1529650337,
      "updated": 1529650669
    },
    {
      "id": "5b2c9c9faff09e0000c68a9b",
      "text": " const sortMode = 'log' // 間隔反復法",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68a9c",
      "text": " let visibleRankNum = 100; // log以外では無視される",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68a9d",
      "text": " const day = 60 * 60 * 24 * 1000",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68a9e",
      "text": " const year = day * 365",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68a9f",
      "text": " let projectPages;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa0",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa1",
      "text": " function get_data(){",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa2",
      "text": "   let xhr = new XMLHttpRequest()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa3",
      "text": "   xhr.open('GET', `/api/pages/${scrapbox.Project.name}?limit=10000`)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa4",
      "text": "   xhr.onload = (e) => {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa5",
      "text": "     projectPages = JSON.parse(xhr.responseText).pages",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa6",
      "text": "     sort_pages()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa7",
      "text": "     update_list()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa8",
      "text": "   }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aa9",
      "text": "   xhr.send(null)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aaa",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aab",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aac",
      "text": " function sort_pages(){",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aad",
      "text": "   const now = Date.now()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aae",
      "text": "   projectPages.forEach((page, index, pages) => {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aaf",
      "text": "     let diff = now - page['updated'] * 1000",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab0",
      "text": "     if(diff > 10 * day){",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab1",
      "text": "       page['score'] = (Math.log(diff / (10 * day)) / Math.log(2)) % 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab2",
      "text": "       page['ago'] = Math.floor(diff / day)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab3",
      "text": "     }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab4",
      "text": "   })",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab5",
      "text": "   projectPages = projectPages.filter(page => (page['score'] != null))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab6",
      "text": "   projectPages.sort((a, b) => (a['score'] - b['score']))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab7",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab8",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ab9",
      "text": " function create_page(title, lines){",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aba",
      "text": "   const projectRoot = (() => {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68abb",
      "text": "     const tmp = location.href.split('/')",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68abc",
      "text": "     tmp.pop()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68abd",
      "text": "     return tmp.join('/')",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68abe",
      "text": "   })()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68abf",
      "text": "   const body = encodeURIComponent(lines.join('\\n'))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac0",
      "text": "   window.open(`${projectRoot}/${title}?body=${body}`)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac1",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac2",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac3",
      "text": " function update_list(){",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac4",
      "text": "   scrapbox.PageMenu('Scrapbox Sort').removeAllItems()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac5",
      "text": "   let page_lists = [];",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac6",
      "text": "   for (let i = 0; i < visibleRankNum; i++) {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac7",
      "text": "     let page = projectPages[i]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac8",
      "text": "     if(page == null) break;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ac9",
      "text": "     page_lists.push(`${strftime(new Date(page.updated * 1000))}(${page['ago']}) [${page['title']}]`)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68aca",
      "text": "   }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68acb",
      "text": "   create_page(\"SRS\" + new Date().toISOString().slice(0, 19), page_lists)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68acc",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68acd",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ace",
      "text": " function pad(number) {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68acf",
      "text": "   if (number < 10) {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad0",
      "text": "     return '0' + number;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad1",
      "text": "   }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad2",
      "text": "   return number;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad3",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad4",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad5",
      "text": " function strftime(d){",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad6",
      "text": "   return (",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad7",
      "text": "     d.getUTCFullYear() +",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad8",
      "text": "     '-' + pad(d.getUTCMonth() + 1) +",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ad9",
      "text": "     '-' + pad(d.getUTCDate())",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ada",
      "text": "   )",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68adb",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68adc",
      "text": " scrapbox.PageMenu.addMenu({",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68add",
      "text": "   title: 'Scrapbox SRS2',",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529651184
    },
    {
      "id": "5b2c9c9faff09e0000c68ade",
      "text": "   image: 'https://gyazo.com/11140c8b35b407c5d490a94ec6f2528f/raw',",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68adf",
      "text": "   onClick: () => {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ae0",
      "text": "     scrapbox.PageMenu('Scrapbox SRS').addItem({ title: 'Please wait...', image: null, onClick: () => null })",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ae1",
      "text": "     get_data();",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ae2",
      "text": "     scrapbox.PageMenu('Scrapbox Sort').removeAllItems()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ae3",
      "text": "   }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9c9faff09e0000c68ae4",
      "text": " })",
      "userId": "582e63d27c56960011aff09e",
      "created": 1529650337,
      "updated": 1529650337
    },
    {
      "id": "5b2c9bd6aff09e0000c68a7d",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
