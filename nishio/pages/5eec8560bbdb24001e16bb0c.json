    },
    {
      "id": "5eec8562aff09e0000f7a8b1",
      "text": "\t[Static Range Sum https://judge.yosupo.jp/problem/static_range_sum]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592558946,
      "updated": 1592558964
    },
    {
      "id": "5eec8572aff09e0000f7a8b2",
      "text": "\t[itertools.accumulate] [itertools]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592558963,
      "updated": 1592559850
    },
    {
      "id": "5eec8577aff09e0000f7a8b3",
      "text": "\t[累積和]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592558967,
      "updated": 1592559014
    },
    {
      "id": "5eec8590aff09e0000f7a8b4",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592558992,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f13",
      "text": " from itertools import accumulate",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f14",
      "text": " N, Q = map(int, input().split())",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f15",
      "text": " AS = list(map(int, input().split()))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f16",
      "text": " AS.append(0)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f17",
      "text": " sums = list(accumulate(AS))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f18",
      "text": " for q in range(Q):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f19",
      "text": "     L, R = map(int, input().split())",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f1a",
      "text": "     s = sums[R - 1]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f1b",
      "text": "     if L > 0:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f1c",
      "text": "         s -= sums[L - 1]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88cdaff09e0000457f1d",
      "text": "     print(s)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592559821,
      "updated": 1592559821
    },
    {
      "id": "5eec88c5aff09e0000457f12",
