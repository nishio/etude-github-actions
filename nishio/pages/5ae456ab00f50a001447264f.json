      "created": 1524919017,
      "updated": 1524919025
    },
    {
      "id": "5ae46af1aff09e0000720602",
      "text": "  分配結果の公正",
      "userId": "582e63d27c56960011aff09e",
      "created": 1524919025,
      "updated": 1524919047
    },
    {
      "id": "5ae46b07aff09e0000720603",
      "text": "  分配課程の公正",
      "userId": "582e63d27c56960011aff09e",
      "created": 1524919047,
      "updated": 1524919053
    },
    {
      "id": "5ae46b0daff09e0000720604",
      "text": " ダイバーシティ・マネジメント",
      "userId": "582e63d27c56960011aff09e",
      "created": 1524919053,
      "updated": 1524919066
    },
    {
      "id": "5ae46b1aaff09e0000720605",
      "text": " \t同化・分離・統合",
      "userId": "582e63d27c56960011aff09e",
      "created": 1524919066,
      "updated": 1524919108
    },
    {
      "id": "5ae46b44aff09e0000720606",
      "text": " プロフェッショナル・マネジメント",
      "userId": "582e63d27c56960011aff09e",
      "created": 1524919108,
      "updated": 1524919115
    },
    {
