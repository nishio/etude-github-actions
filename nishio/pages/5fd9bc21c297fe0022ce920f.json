    },
    {
      "id": "5fd9bd6caff09e000037f3c8",
      "text": "　[偉さは一時的]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608105326,
      "updated": 1608105331
    },
    {
      "id": "5fd9bc48aff09e00000b1cc7",
      "text": "　[偉さ]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608105033,
      "updated": 1608105229
    },
    {
      "id": "5fd9bc48aff09e00000b1cc8",
