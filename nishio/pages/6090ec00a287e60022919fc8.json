    {
      "id": "6090ec24aff09e00003d27c7",
      "text": " </comment>",
      "created": 1247065200,
      "updated": 1247065200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec24aff09e00003d27c8",
      "text": " <comment>",
      "created": 1247065200,
      "updated": 1247065200,
      "userId": "582e63d27c56960011aff09e"
