    },
    {
      "id": "6090ec23aff09e00003d0d85",
      "text": " *1240999605*TwitterがGoogleになった",
      "created": 1240930800,
      "updated": 1240930800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d0d86",
      "text": " なんか変なことになった",
      "created": 1240930800,
      "updated": 1240930800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec23aff09e00003d0d87",
      "text": " <img src=\"http://gyazo.com/d89b9a91dd882695e35b2c07882fbe3a.png\">",
      "created": 1240930800,
      "updated": 1240930800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
