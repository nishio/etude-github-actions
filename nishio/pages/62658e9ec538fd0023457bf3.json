    {
      "id": "62659280aff09e00000536a1",
      "text": "requestsでならできる",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823809,
      "updated": 1650823820
    },
    {
      "id": "62659065aff09e000057de6d",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823269,
      "updated": 1650823273
    },
    {
      "id": "62659068aff09e000057de6f",
      "text": " def crawl():",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823273,
      "updated": 1650823273
    },
    {
      "id": "62659069aff09e000057de70",
      "text": "     urls = open(\"urls.txt\").read().strip(\"'\").split()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823273,
      "updated": 1650823846
    },
    {
      "id": "62659069aff09e000057de71",
      "text": "     for url in urls:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823273,
      "updated": 1650823273
    },
    {
      "id": "62659069aff09e000057de72",
      "text": "         print(url)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823273,
      "updated": 1650823273
    },
    {
      "id": "62659069aff09e000057de73",
      "text": "         fn = url.replace(PREFIX, \"\")",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823273,
      "updated": 1650823273
    },
    {
      "id": "62659069aff09e000057de74",
      "text": "         r = requests.get(url)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823273,
      "updated": 1650823273
    },
    {
      "id": "62659069aff09e000057de75",
      "text": "         print(r)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823273,
      "updated": 1650823273
    },
    {
      "id": "62659069aff09e000057de76",
      "text": "         open(f\"data/{fn}.html\", \"w\").write(r.text)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1650823273,
      "updated": 1650823273
    },
    {
      "id": "62659069aff09e000057de77",
