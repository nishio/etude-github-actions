      "created": 1525442806,
      "updated": 1525442811
    },
    {
      "id": "5aec68f6aff09e0000c71240",
      "text": "  コミュニケーション",
      "userId": "582e63d27c56960011aff09e",
      "created": 1525442806,
      "updated": 1525442806
    },
    {
      "id": "5aec68f6aff09e0000c71241",
      "text": "  シンプル",
      "userId": "582e63d27c56960011aff09e",
      "created": 1525442806,
      "updated": 1525442806
    },
    {
      "id": "5aec68f6aff09e0000c71242",
      "text": "  フィードバック",
