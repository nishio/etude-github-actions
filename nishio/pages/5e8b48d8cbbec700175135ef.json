    {
      "id": "5e8b4944aff09e000096ae13",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186564,
      "updated": 1586186574
    },
    {
      "id": "5e8b494eaff09e000096ae15",
      "text": " from skimage import filters",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186574,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae16",
      "text": " from skimage import io",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae17",
      "text": " import numpy as np",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae18",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae19",
      "text": " WIDTH = 480",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae1a",
      "text": " HEIGHT = 360",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae1b",
      "text": " T = 20",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae1c",
      "text": " data = np.zeros((HEIGHT, WIDTH, 3))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae1d",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae1e",
      "text": " N = 75",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae1f",
      "text": " data[HEIGHT // 2, WIDTH // 2] = (0, N * N, 0)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae20",
      "text": " data = filters.gaussian(data, sigma=N, multichannel=True)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae21",
      "text": " data /= data.max()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae22",
      "text": " data *= 255",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae23",
      "text": " data *= T",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae24",
      "text": " data = data.astype(np.uint8)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
      "id": "5e8b4954aff09e000096ae25",
      "text": " io.imsave(\"tmp.png\", data)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1586186581,
      "updated": 1586186581
    },
    {
