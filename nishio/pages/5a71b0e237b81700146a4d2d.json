  "lines": [
    {
      "id": "5a71b0e237b81700146a4d2d",
      "text": "モンハン",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400293,
      "updated": 1517400293
    },
    {
      "id": "5a71b0e5aff09e0000a15643",
      "text": "妻がモンハンをしているのを見て思った",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400293,
      "updated": 1517400302
    },
    {
      "id": "5a71b13faff09e0000a1564c",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400383,
      "updated": 1517400383
    },
    {
      "id": "5a71b0eeaff09e0000a15645",
      "text": "\tマップを探索して資源を集める",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400302,
      "updated": 1517400309
    },
    {
      "id": "5a71b0f5aff09e0000a15646",
      "text": " \t書籍などから情報を集める",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400309,
      "updated": 1517400328
    },
    {
      "id": "5a71b108aff09e0000a15647",
      "text": " 資源を加工して装備を作る",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400328,
      "updated": 1517400333
    },
    {
      "id": "5a71b10caff09e0000a15648",
      "text": " \t情報を加工して戦うための知識を作る",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400333,
      "updated": 1517400362
    },
    {
      "id": "5a71b12aaff09e0000a15649",
      "text": " モンスターと戦う",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400362,
      "updated": 1517400366
    },
    {
      "id": "5a71b12eaff09e0000a1564a",
      "text": " \t知識を使って課題を解決する",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400366,
      "updated": 1517400377
    },
    {
      "id": "5a71b13aaff09e0000a1564b",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400378,
      "updated": 1517400378
    },
    {
      "id": "5a71b0eaaff09e0000a15644",
      "text": "だいたいモンハンと同じだな",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400298,
      "updated": 1517400394
    },
    {
      "id": "5a71b146aff09e0000a1564d",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1517400390,
      "updated": 1517400390
    }
  ]
}