      "created": 1208876400,
      "updated": 1208876400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9a0e",
      "text": "     string result = \"\";",
      "created": 1208876400,
      "updated": 1208876400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9a0f",
      "text": " ",
      "created": 1208876400,
      "updated": 1208876400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9a10",
      "text": "     if('0' <= c && c <= '9'){",
      "created": 1208876400,
      "updated": 1208876400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9a11",
