    },
    {
      "id": "6090ec1daff09e00003c9a0a",
      "text": "   string decodingEncoding(string code, string message) {",
      "created": 1208876400,
      "updated": 1208876400,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9a0b",
