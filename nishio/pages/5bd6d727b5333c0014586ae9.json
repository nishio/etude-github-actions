    {
      "id": "5bd6d727aff09e000076a192",
      "text": "常にこちらを向かせる",
      "created": 1529600631,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a193",
      "text": "[https://gyazo.com/b4fc98c314302a46476e2f4d1f1649fe]",
      "created": 1529600703,
      "updated": 1529600703,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a194",
      "text": "",
      "created": 1529600659,
      "updated": 1529600659,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a195",
      "text": "\t[Quaternion.LookRotation]で簡単に作れる",
      "created": 1529600637,
      "updated": 1529600651,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a196",
      "text": "code::",
      "created": 1529600637,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a197",
      "text": " for(int i = 0; i < slides.Count; i++)",
      "created": 1529600637,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a198",
      "text": " {",
      "created": 1529600637,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a199",
      "text": "     GameObject obj = slides[i] as GameObject;",
      "created": 1529600637,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a19a",
      "text": "     Vector3 relativePos = obj.transform.position - camera.transform.position;",
      "created": 1529600637,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a19b",
      "text": "     Quaternion rotation = Quaternion.LookRotation(relativePos, camera.transform.up);",
      "created": 1529600637,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a19c",
      "text": "     obj.transform.rotation = rotation;",
      "created": 1529600637,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a19d",
      "text": " }",
      "created": 1529600637,
      "updated": 1529600637,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "5bd6d727aff09e000076a19e",
