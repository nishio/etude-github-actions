    {
      "id": "5ef01868aff09e0000e60bca",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793192,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bcd",
      "text": " import sys",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bce",
      "text": " import numba",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bcf",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bd0",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bd1",
      "text": " def recur(t):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793204
    },
    {
      "id": "5ef0186daff09e0000e60bd3",
      "text": "     if t == 0:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bd4",
      "text": "         return 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bd5",
      "text": "     return t * recur(t - 1)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bd6",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bd7",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bd8",
      "text": " if sys.argv[-1] == \"-c\":",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bd9",
      "text": "     print(\"compiling\")",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793227
    },
    {
      "id": "5ef0186daff09e0000e60bdb",
      "text": "     from numba.pycc import CC",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bdc",
      "text": "     cc = CC('my_module')",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bdd",
      "text": "     cc.export('recur', 'i8(i8)')(recur)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60bde",
      "text": "     cc.compile()",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793210
    },
    {
      "id": "5ef0186daff09e0000e60be0",
      "text": " else:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793212
    },
    {
      "id": "5ef0186daff09e0000e60be2",
      "text": "     from my_module import recur",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60be3",
      "text": "     print(recur(5))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793198
    },
    {
      "id": "5ef0186daff09e0000e60be4",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793198,
      "updated": 1592793215
    },
    {
      "id": "5ef01869aff09e0000e60bcb",
      "text": "code::",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793193,
      "updated": 1592793233
    },
    {
      "id": "5ef01891aff09e0000e60be7",
      "text": " Untyped global name 'recur': cannot determine Numba type of <class 'function'>",
      "userId": "582e63d27c56960011aff09e",
      "created": 1592793234,
      "updated": 1592793248
    },
    {
      "id": "5ef018a0aff09e0000e60be8",
      "text": " ",
