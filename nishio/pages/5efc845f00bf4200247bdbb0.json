    },
    {
      "id": "5efc845faff09e00004dc011",
      "text": "1TLE",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc012",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc013",
      "text": " def solve(N, M, edges):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc014",
      "text": "     longest = [-1] * (N + 1)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc015",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc016",
      "text": "     def get_longest(start):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc017",
      "text": "         if longest[start] != -1:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc018",
      "text": "             return longest[start]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc019",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc01a",
      "text": "         next_edges = edges.get(start)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc01b",
      "text": "         if not next_edges:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc01c",
      "text": "             ret = 0",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc01d",
      "text": "         else:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc01e",
      "text": "             ret = max(get_longest(v) for v in edges[start]) + 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc01f",
      "text": "         longest[start] = ret",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc020",
      "text": "         return ret",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc021",
      "text": " ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc022",
      "text": "     return max(get_longest(v) for v in edges)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc023",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc024",
      "text": "Python 428 ms AC",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc025",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc026",
      "text": " def solve(N, M, edges):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc027",
      "text": "     longest = [-1] * (N + 1)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc028",
      "text": "  ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc029",
      "text": "     for i in range(N + 1):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc02a",
      "text": "         if not edges[i]:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc02b",
      "text": "             longest[i] = 0",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc02c",
      "text": "  ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc02d",
      "text": "     def get_longest(start):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc02e",
      "text": "         next = edges[start]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc02f",
      "text": "         for v in next:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc030",
      "text": "             if longest[v] == -1:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc031",
      "text": "                 longest[v] = get_longest(v)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc032",
      "text": "  ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc033",
      "text": "         ret = max(longest[v] for v in next) + 1",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc034",
      "text": "         return ret",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc035",
      "text": "  ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc036",
      "text": "     for i in range(N + 1):",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc037",
      "text": "         if longest[i] == -1:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc038",
      "text": "             longest[i] = get_longest(i)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc039",
      "text": "  ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
    {
      "id": "5efc845faff09e00004dc03a",
      "text": "     return max(longest[v] for v in edges)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1593607264,
      "updated": 1593607264
    },
