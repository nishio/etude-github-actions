      "created": 1594541933,
      "updated": 1594541974
    },
    {
      "id": "5f0ac796aff09e0000eb7231",
      "text": "\t\t\t In [94]: fs[::2]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1594541974,
      "updated": 1594541974
    },
    {
      "id": "5f0ac796aff09e0000eb7232",
      "text": "    Out[94]: ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1594541974,
      "updated": 1594541974
    },
    {
      "id": "5f0ac796aff09e0000eb7233",
      "text": "    array([   0,    2,    6,   12,   20,   30,   42,   56,   72,   90,  110,",
      "userId": "582e63d27c56960011aff09e",
      "created": 1594541974,
