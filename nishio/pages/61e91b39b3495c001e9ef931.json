    },
    {
      "id": "61e91b39aff09e0000d0b086",
      "text": "　[(0.2.1) Collect information concretely]",
      "created": 1546930917,
      "updated": 1553661484,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "61e91b39aff09e0000d0b087",
      "text": "\t[(0.2.2) Compare and find patterns]",
      "created": 1546930917,
      "updated": 1553660015,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "61e91b39aff09e0000d0b088",
      "text": "\t[(0.2.3) Practice and verification]",
      "created": 1546931068,
      "updated": 1553661478,
      "userId": "582e63d27c56960011aff09e"
    },
    {
