    {
      "id": "5bc1bc10ff01780013d4a53b",
      "text": "切り離し",
      "userId": "582e63d27c56960011aff09e",
      "created": 1539423253,
      "updated": 1539423253
    },
    {
      "id": "5bc1bc15aff09e0000781864",
      "text": " 異なる二つの側面が",
      "userId": "582e63d27c56960011aff09e",
      "created": 1539423253,
      "updated": 1539423272
    },
    {
      "id": "5bc1bc2daff09e0000781866",
      "text": " 歴史的経緯などの理由で",
      "userId": "582e63d27c56960011aff09e",
      "created": 1539423277,
      "updated": 1539423287
    },
    {
      "id": "5bc1bc38aff09e0000781867",
      "text": " 一つに結び付けられている",
      "userId": "582e63d27c56960011aff09e",
      "created": 1539423288,
      "updated": 1539423301
    },
    {
      "id": "5bc1bc4aaff09e0000781868",
      "text": " それが歪みを生んでいるのなら",
      "userId": "582e63d27c56960011aff09e",
      "created": 1539423306,
      "updated": 1539423320
    },
    {
      "id": "5bc1bc58aff09e0000781869",
      "text": " 切り離さなければならない",
      "userId": "582e63d27c56960011aff09e",
      "created": 1539423320,
      "updated": 1539423327
    },
    {
      "id": "5bc1bc61aff09e000078186b",
