      "created": 1271602800,
      "updated": 1271602800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec27aff09e00003d592b",
      "text": " CCSprite* ball = [CCSprite spriteWithFile:@\"r1.png\"];",
      "created": 1271602800,
      "updated": 1271602800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec27aff09e00003d592c",
      "text": " ball.position = ccp(240, 160);",
      "created": 1271602800,
      "updated": 1271602800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec27aff09e00003d592d",
      "text": " [self addChild:ball];",
      "created": 1271602800,
      "updated": 1271602800,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec27aff09e00003d592e",
