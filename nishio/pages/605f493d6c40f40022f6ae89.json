  "lines": [
    {
      "id": "605f493d6c40f40022f6ae89",
      "text": "HackMD",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616857408,
      "updated": 1616857408
    },
    {
      "id": "605f4f6caff09e000041d2b1",
      "text": "軽量な[共同編集ツール]。",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616858989,
      "updated": 1616859200
    },
    {
      "id": "605f4fa9aff09e000041d2b2",
      "text": "URLで共有して、アカウントがない人にもwrite権限を与えられる",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616859050,
      "updated": 1616859091
    },
    {
      "id": "605f4fdeaff09e000041d2b3",
      "text": "編集モードはWYSIWYGではなく、Markdownエディタとリアルタイムプレビューになる",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616859102,
      "updated": 1616859170
    },
    {
      "id": "605f4f69aff09e00009929b8",
      "text": "https://hackmd.io/",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616858986,
      "updated": 1616858986
    },
    {
      "id": "605f502faff09e000041d2b4",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616859183,
      "updated": 1616859183
    },
    {
      "id": "605f5045aff09e000041d2b5",
      "text": "Scrapboxと比べた長所",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616859206,
      "updated": 1616859216
    },
    {
      "id": "605f5052aff09e000041d2b7",
      "text": "　編集にアカウントが必要ない",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616859218,
      "updated": 1616859229
    },
    {
      "id": "605f5066aff09e000041d2b8",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616859239,
      "updated": 1616859239
    },
    {
      "id": "605f5067aff09e000041d2b9",
      "text": "Google Docsと比べた長所",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616859239,
      "updated": 1616859258
    },
    {
      "id": "605f5079aff09e000041d2ba",
      "text": "　多人数がwriteしたときに壊れにくいという説がある",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616859258,
      "updated": 1616859275
    },
    {
      "id": "606a9be4aff09e0000c5fad5",
      "text": "　スマホからでも使いやすい",
      "userId": "582e63d27c56960011aff09e",
      "created": 1617599461,
      "updated": 1617599469
    },
    {
      "id": "606a9bf1aff09e0000c5fad6",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1617599473,
      "updated": 1617599473
    },
    {
      "id": "606a9bf1aff09e0000c5fad7",
      "text": "短所",
      "userId": "582e63d27c56960011aff09e",
      "created": 1617599473,
      "updated": 1617599478
    },
    {
      "id": "606a9bf5aff09e0000c5fad8",
      "text": "　表示モードと編集モードの切り替えがピンとこない人がいる",
      "userId": "582e63d27c56960011aff09e",
      "created": 1617599478,
      "updated": 1617599503
    },
    {
      "id": "606a9c1baff09e0000c5fad9",
      "text": "　Markdownに不慣れな人には使いづらい",
      "userId": "582e63d27c56960011aff09e",
      "created": 1617599515,
      "updated": 1617599555
    },
