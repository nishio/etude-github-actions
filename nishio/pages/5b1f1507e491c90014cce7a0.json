    {
      "id": "5b1f1513aff09e0000e57770",
      "text": "Feeling of Knowing in Memory and Problem Solving",
      "userId": "582e63d27c56960011aff09e",
      "created": 1528763668,
      "updated": 1528763679
    },
    {
      "id": "5b1f1513aff09e0000e57771",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1528763668,
