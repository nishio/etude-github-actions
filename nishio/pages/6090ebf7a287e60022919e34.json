      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d9a",
      "text": " ",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d9b",
      "text": " >|cpp|",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d9c",
      "text": " template<typename K, typename V>",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d9d",
      "text": "   void print_map2(map<K, V> m){",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d9e",
      "text": "   cout << \"{\";",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d9f",
      "text": "   for(typename map<K, V>::iterator it=m.begin(); it != m.end(); ++it){",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9da0",
      "text": "     if(it != m.begin()) cout << \", \";",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9da1",
      "text": "     cout << (*it).first << \":\" << (*it).second;",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9da2",
      "text": "   }",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9da3",
      "text": "   cout << \"}\" << endl;",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9da4",
      "text": " }",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9da5",
      "text": " ||<",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9da6",
      "text": " ",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9da7",
      "text": " 「for(typename」が重要。",
