      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9db8",
      "text": "   int xs[] = {1, 2, 3}, ys[] = {10, 20, 30};",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9db9",
      "text": "   map<int, int> m = mapzip(xs, ys); ",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9dba",
      "text": "   print_map2(m);",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9dbb",
      "text": " }",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9dbc",
      "text": " ||<",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
