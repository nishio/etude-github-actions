      "text": " -----",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d82",
      "text": " ",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d83",
      "text": " >|cpp|",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d84",
      "text": " template<class T1, class T2>",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d85",
      "text": " map<T1, T2> mapzip(T1 keys[], T2 values[], size_t N){",
      "created": 1209481200,
      "updated": 1209481200,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec1daff09e00003c9d86",
      "text": "   map<T1, T2> m;",
