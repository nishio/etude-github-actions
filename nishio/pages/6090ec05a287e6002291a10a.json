    },
    {
      "id": "6090ec29aff09e00003d756e",
      "text": " ",
      "created": 1285686000,
      "updated": 1285686000,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d756f",
      "text": " よいまとめ！",
      "created": 1285686000,
      "updated": 1285686000,
      "userId": "582e63d27c56960011aff09e"
    },
    {
      "id": "6090ec29aff09e00003d7570",
      "text": " ",
