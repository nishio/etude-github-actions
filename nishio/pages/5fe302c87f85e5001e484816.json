  "lines": [
    {
      "id": "5fe302c87f85e5001e484816",
      "text": "Best Kの取得",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d95",
      "text": "from [heapq]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d96",
      "text": "Best Kの取得",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d97",
      "text": "\tサイズがkを超えたらpop",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d98",
      "text": "code:python",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d99",
      "text": " from random import shuffle",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d9a",
      "text": " xs = list(range(100))",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d9b",
      "text": " shuffle(xs)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d9c",
      "text": " K = 3",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d9d",
      "text": " queue = xs[:K]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d9e",
      "text": " heapify(queue)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4d9f",
      "text": " for x in xs[K:]:",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4da0",
      "text": "     heappush(queue, x)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4da1",
      "text": "     heappop(queue)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
      "id": "5fe302c8aff09e00003d4da2",
      "text": " print(queue)  # => [97, 98, 99]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1608712905,
      "updated": 1608712905
    },
    {
