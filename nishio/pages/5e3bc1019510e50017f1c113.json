  "lines": [
    {
      "id": "5e3bc1019510e50017f1c113",
      "text": "TypeScript Memo",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974339,
      "updated": 1580974398
    },
    {
      "id": "5e3bc104aff09e000091faff",
      "text": "code:ts",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974340,
      "updated": 1580974343
    },
    {
      "id": "5e3bc107aff09e000091fb01",
      "text": " let state: ({ type: \"INITIAL\", value: null } | { type: \"WITH_NUMBER\", value: number })",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb02",
      "text": " const f = () => {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb03",
      "text": "   if (state.type === \"WITH_NUMBER\") {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb04",
      "text": "     let x: number = state.value;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb05",
      "text": "   }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb06",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb07",
      "text": " const f2 = () => {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb08",
      "text": "   const type = state.type;",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb09",
      "text": "   if (type === \"WITH_NUMBER\") {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb0a",
      "text": "     // let x: number = state.value;  // NG",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb0b",
      "text": "     // Type 'number | null' is not assignable to type 'number'.",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb0c",
      "text": "   }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb0d",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974347
    },
    {
      "id": "5e3bc107aff09e000091fb0e",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974343,
      "updated": 1580974345
    },
    {
      "id": "5e3bc104aff09e000091fb00",
      "text": "code:ts",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974341,
      "updated": 1580974512
    },
    {
      "id": "5e3bc1e7aff09e000091fb18",
      "text": " // before",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974567,
      "updated": 1580974569
    },
    {
      "id": "5e3bc1b0aff09e000091fb10",
      "text": " if (dragStartType === \"RootPiece\") {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974513,
      "updated": 1580974513
    },
    {
      "id": "5e3bc1b1aff09e000091fb11",
      "text": "   if (target !== null && isPiecePaperItem(target)) {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974513,
      "updated": 1580974518
    },
    {
      "id": "5e3bc1b1aff09e000091fb12",
      "text": "     updatePiecePosition(target);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974513,
      "updated": 1580974518
    },
    {
      "id": "5e3bc1b1aff09e000091fb13",
      "text": "     deselect(target);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974513,
      "updated": 1580974518
    },
    {
      "id": "5e3bc1b1aff09e000091fb14",
      "text": "   } else {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974513,
      "updated": 1580974518
    },
    {
      "id": "5e3bc1b1aff09e000091fb15",
      "text": "     throw new Error(\"never\");",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974513,
      "updated": 1580974518
    },
    {
      "id": "5e3bc1b1aff09e000091fb16",
      "text": "   }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974513,
      "updated": 1580974518
    },
    {
      "id": "5e3bc1b8aff09e000091fb17",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974520,
      "updated": 1580974522
    },
    {
      "id": "5e3bc1ebaff09e000091fb19",
      "text": " //after",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974572,
      "updated": 1580974573
    },
    {
      "id": "5e3bc1edaff09e000091fb1a",
      "text": " if (mouseState.type === \"RootPiece\") {",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974573,
      "updated": 1580974586
    },
    {
      "id": "5e3bc1edaff09e000091fb1b",
      "text": "   updatePiecePosition(mouseState.target);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974574,
      "updated": 1580974576
    },
    {
      "id": "5e3bc1edaff09e000091fb1c",
      "text": "   deselect(mouseState.target);",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974574,
      "updated": 1580974578
    },
    {
      "id": "5e3bc1edaff09e000091fb1d",
      "text": " }",
      "userId": "582e63d27c56960011aff09e",
      "created": 1580974574,
      "updated": 1580974578
    },
