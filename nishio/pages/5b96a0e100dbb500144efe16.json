      "text": "code::",
      "userId": "582e63d27c56960011aff09e",
      "created": 1536599376,
      "updated": 1536599386
    },
    {
      "id": "5b96a551aff09e0000456c36",
      "text": "\tEnter at least one positive examples (empty to exit)",
      "userId": "582e63d27c56960011aff09e",
      "created": 1536599377,
      "updated": 1536599386
    },
    {
      "id": "5b96a54faff09e0000456c34",
      "text": "\t> ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1536599376,
      "updated": 1536599387
    },
    {
      "id": "5b96a5d6aff09e0000456c38",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1536599510,
      "updated": 1536599510
    },
    {
