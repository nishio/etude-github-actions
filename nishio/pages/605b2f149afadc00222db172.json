      "userId": "582e63d27c56960011aff09e",
      "created": 1616589381,
      "updated": 1616589381
    },
    {
      "id": "605b3245aff09e000083efe5",
      "text": "               type: Scratch.ArgumentType.NUMBER,",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616589381,
      "updated": 1616589381
    },
    {
      "id": "605b3245aff09e000083efe6",
      "text": "               defaultValue: '3' ",
      "userId": "582e63d27c56960011aff09e",
      "created": 1616589381,
      "updated": 1616589381
    },
