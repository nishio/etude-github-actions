    {
      "id": "5a65ba89a1bd27001405f457",
      "text": "How to Start a d.school",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516616334,
      "updated": 1516617235
    },
    {
      "id": "5a65bcb2aff09e0000649c05",
      "text": "[How to Start a d.school — Stanford d.school https://dschool.stanford.edu/how-to-start-a-dschool/]",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516616882,
      "updated": 1516617229
    },
    {
      "id": "5a65be0daff09e0000649c07",
      "text": "10要件",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617229,
      "updated": 1516617239
    },
    {
      "id": "5a65be16aff09e0000649c09",
      "text": " 1 Be radically student-centered",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617239,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c0a",
      "text": " 2 Embrace clashing perspectives",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c0b",
      "text": " 3 Show unfinished work",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c0c",
      "text": " 4 Focus on the how, not the what",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c0d",
      "text": " 5 Seek out fresh minds",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c0e",
      "text": " 6 Allow people to opt in",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c0f",
      "text": " 7 Build in room for change",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c10",
      "text": " 8 Remember learning is a designed activity",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c11",
      "text": " 9 Find a balance between chaos and control",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be2faff09e0000649c12",
      "text": " 10 Pay attention to team dynamics",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617264,
      "updated": 1516617264
    },
    {
      "id": "5a65be14aff09e0000649c08",
      "text": "",
      "userId": "582e63d27c56960011aff09e",
      "created": 1516617237,
      "updated": 1516617237
    }
  ]
}